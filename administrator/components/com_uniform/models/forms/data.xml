<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field 
			name="data_id" type="hidden" default="0" 
			label="JSN_UNIFORM_DATA_ID" 
		/>
		<field
			name="form_id"
			type="text"
			label="JSN_UNIFORM_FORM_ID"
			description="JSN_UNIFORM_FORM_ID_DES"
			class="inputbox"
			size="110"
			/>
		<field
			name="user_id"
			type="text"
			label="JSN_UNIFORM_USER_ID"
			description="JSN_UNIFORM_USER_ID_DES"
			rows="4"
			cols="110"/>
		<field
			name="data_ip"
			type="text"
			label="JSN_UNIFORM_DATA_IP"
			description="JSN_UNIFORM_DATA_IP_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_country"
			type="text"
			label="JSN_UNIFORM_DATA_COUNTRY"
			description="JSN_UNIFORM_DATA_COUNTRY_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_country_code"
			type="text"
			default="0"
			label="JSN_UNIFORM_DATA_COUNTRY_CODE"
			description ="JSN_UNIFORM_DATA_COUNTRY_CODE_DES"
			class="readonly"
			readonly="true"/>
		<field
			name="data_browser"
			type="text"
			label="JSN_UNIFORM_DATA_BROWSER"
			description="JSN_UNIFORM_DATA_BROWSER_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_browser_version"
			type="text"
			label="JSN_UNIFORM_DATA_BROWSER_VERSION"
			description="JSN_UNIFORM_DATA_BROWSER_VERSION_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_browser_agent"
			type="text"
			label="JSN_UNIFORM_DATA_BROWSER_AGENT"
			description="JSN_UNIFORM_DATA_BROWSER_AGENT_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_os"
			type="text"
			label="JSN_UNIFORM_DATA_OS"
			description="JSN_UNIFORM_DATA_OS_DES"
			class="inputbox"
			size="40"
			/>
		<field
			name="data_state"
			type="list"
			class="inputbox"
			default="1"
			size="1"
			label="JSN_UNIFORM_DATA_STATE"
			description="JSN_UNIFORM_DATA_STATE_DES">
			<option
				value="1">
				JPUBLISHED
			</option>
			<option
				value="0">
				JUNPUBLISHED
			</option>
			<option
				value="2">
				JARCHIVED
			</option>
			<option
				value="-2">
				JTRASHED
			</option>
		</field>
		<field
			name="data_created_by"
			type="text"
			label="JSN_UNIFORM_DATA_CREATE_BY"
			desc="JSN_UNIFORM_DATA_CREATE_BY_DES"
		/>
		<field
			name="data_created_at"
			type="text"
			label="JSN_UNIFORM_DATA_CREATE_AT"
			desc="JSN_UNIFORM_DATA_CREATE_AT_DES"
			class="readonly"
			filter="unset"
			readonly="true" />
		
	</fieldset>
        
</form>
